<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.serviceprovider.mapper.UserInfoMapper">

    <select id="queryList" resultType="org.example.serviceprovider.entity.UserEntity">
        select * from user where id =#{id}
    </select>

    <insert id="insertUser">
        insert into user(id, name, password, age, email) values(#{id},#{name},#{password},#{age},#{email})
    </insert>
</mapper>